<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav"
    app:startDestination="@id/splashScreen">

    <fragment
        android:id="@+id/splashScreen"
        android:name="com.games.types7.app.SplashScreen"
        android:label="SplashScreen" >
        <action
            android:id="@+id/action_splashScreen_to_fragmentWebView"
            app:destination="@id/fragmentWebView" />
        <action
            android:id="@+id/action_splashScreen_to_sportFragment"
            app:destination="@id/sportFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentWebView"
        android:name="com.games.types7.app.FragmentWebView"
        android:label="FragmentWebView" />
    <fragment
        android:id="@+id/sportFragment"
        android:name="com.games.types7.app.SportFragment"
        android:label="SportFragment" />
</navigation>